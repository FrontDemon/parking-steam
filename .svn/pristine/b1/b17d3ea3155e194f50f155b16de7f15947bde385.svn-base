.foot {
	clear: both;
	position: relative;
	top: 20px;
}

.layui-nav-item {
	background-color: #F7F7F7 !important;
}

.layui-nav-item a {
	color: black !important;
}

.layui-nav-item a:hover {
	background-color: #DEDEDE !important;
}