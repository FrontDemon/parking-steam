package com.smdm.statusenum;
/**
 * orders的状态信息表
 * @author manRED
 *
 */
public enum OrderEnum implements CodeEnum{
	ORDER_PAID(1,"已支付"),
	ORDER_UNPAID(0,"未支付"),
	;
	private int code;
	private String msg;
	private OrderEnum(int code, String msg) {
		this.code = code;
		this.msg = msg;
	}
	public int getCode() {
		return code;
	}
	public String getMsg() {
		return msg;
	}
	
	
}
